
@{
    ViewData["Title"] = "RegisteredCustomers";
}

@model List<ApplicationUser>

<h1>Registered Customers</h1>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
                <tr>
                    <td>@customer.UserName</td>
                    <td>@customer.Email</td>
                </tr>
        }
    </tbody>
</table>
